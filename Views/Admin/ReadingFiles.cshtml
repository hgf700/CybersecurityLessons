@model List<string>

<h2>Dostępne pliki </h2>

@if (Model.Count == 0)
{
    <p>Brak plików </p>
}
else
{
    <ul>
        @foreach (var file in Model)
        {
            <li>
                <a href="@Url.Action("DownloadFile", "Admin", new { fileName = file })">
                    @file
                </a>
            </li>
        }
    </ul>
}
